<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<Description>MCART WPF Library</Description>
		<AssemblyName>MCART.WPF</AssemblyName>
		<OutputType>Library</OutputType>
		<TargetFramework>net5.0-Windows</TargetFramework>
		<UseWPF>true</UseWPF>
	</PropertyGroup>
	<ItemGroup>
		<None Include="Art\**" />
		<Page Remove="Art\**" />
		<Compile Remove="Art\**" />
		<EmbeddedResource Remove="Art\**" />
		<None Remove="Resources\Icons\**" />
		<EmbeddedResource Include="Resources\Icons\**" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="$(SolutionDir)Lib\MCART-Windows\MCART-Windows.csproj" />
		<ProjectReference Include="$(SolutionDir)Lib\MCART-GUI\MCART.GUI.csproj" />
		<ProjectReference Include="$(SolutionDir)Lib\MCART-UI\MCART.UI.csproj" />
		<ProjectReference Include="$(SolutionDir)MCART\MCART.csproj" />
	</ItemGroup>
	<ItemGroup>
		<Compile Update="Resources\Strings\Errors.Designer.cs">
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
			<DependentUpon>Errors.resx</DependentUpon>
		</Compile>
		<Compile Update="Resources\Strings\Common.Designer.cs">
		  <DesignTime>True</DesignTime>
		  <AutoGen>True</AutoGen>
		  <DependentUpon>Common.resx</DependentUpon>
		</Compile>
	</ItemGroup>
	<ItemGroup>
		<EmbeddedResource Update="Resources\Strings\Errors.resx">
			<Generator>ResXFileCodeGenerator</Generator>
			<LastGenOutput>Errors.Designer.cs</LastGenOutput>
			<CustomToolNamespace>TheXDS.MCART.Wpf.Resources.Strings</CustomToolNamespace>
		</EmbeddedResource>
		<EmbeddedResource Update="Resources\Strings\Common.resx">
		  <Generator>PublicResXFileCodeGenerator</Generator>
		  <LastGenOutput>Common.Designer.cs</LastGenOutput>
		  <CustomToolNamespace>TheXDS.MCART.Wpf.Resources.Strings</CustomToolNamespace>
		</EmbeddedResource>
		<EmbeddedFiles Include="Dialogs/*" />
		<EmbeddedFiles Include="Pages/*" />
	</ItemGroup>
</Project>