<Project Sdk="Microsoft.NET.Sdk" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">  
  <PropertyGroup>
    <TargetFramework>net5.0-windows</TargetFramework>
    <Description>MCART for Windows</Description>
    <AssemblyName>MCART.Windows</AssemblyName>
    <RootNamespace>TheXDS.MCART.Windows</RootNamespace>
  </PropertyGroup>  
  <ItemGroup>
    <PackageReference Include="Microsoft.Win32.Registry" Version="5.0.0" />
    <PackageReference Include="System.Drawing.Common" Version="6.0.0" />
    <PackageReference Include="System.Management" Version="6.0.0" />
  </ItemGroup>  
  <ItemGroup>
    <ProjectReference Include="..\MCART-GUI\MCART.GUI.csproj" />
  </ItemGroup>  
  <ItemGroup>
    <Compile Update="Resources\Strings\Errors.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Errors.resx</DependentUpon>
    </Compile>
  </ItemGroup>  
  <ItemGroup>
    <EmbeddedResource Update="Resources\Strings\Errors.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Errors.Designer.cs</LastGenOutput>
      <CustomToolNamespace>TheXDS.MCART.Windows.Resources.Strings</CustomToolNamespace>
    </EmbeddedResource>
  </ItemGroup>
</Project>