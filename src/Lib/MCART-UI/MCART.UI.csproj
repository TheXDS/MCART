<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Description>MCART interactive components</Description>
    <RootNamespace>TheXDS.MCART</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <Compile Update="Resources\Strings\UiErrors.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>UiErrors.resx</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="Resources\Strings\UiErrors.en-us.resx">
      <CustomToolNamespace>TheXDS.MCART.Resources.UI</CustomToolNamespace>
      <LastGenOutput>UiErrors.en-us.Designer.cs</LastGenOutput>
      <Generator>ResXFileCodeGenerator</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\Strings\UiErrors.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>UiErrors.Designer.cs</LastGenOutput>
      <CustomToolNamespace></CustomToolNamespace>
    </EmbeddedResource>
  </ItemGroup>
</Project>